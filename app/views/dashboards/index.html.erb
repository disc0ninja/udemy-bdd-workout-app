<div class="mdl-grid">
  <div class="mdl-card mdl-shadow--3dp
    mdl-cell--4-col-desktop mdl-cell--8-offset-desktop">
    <div class="mdl-card__title">
      <h3 class="mdl-card__title-text">Workout Lounge</h3>
    </div>
    <div class="mdl-card__supporting-text">
      Share your workout with friends and family
    </div>
  </div>
</div>
<div class="mdl-grid">
  <div class="mdl-cell mdl-card mdl-shadow--3dp mdl-cell--6-col mdl-cell--3-offset-desktop">
    <div class="mdl-card__title">
      <h3 class="mdl-card__title-text">List of Members</h3>
    </div>
    <% @athletes.each do |athlete| %>
      <div class="mdl-card mdl-shadow--2dp">
        <div class="mdl-card__title">
          <h5 class="mdl-car__title-text">
          <%=  athlete.full_name %>
          </h5>
        </div>
      </div>
    <% end %>
  </div>

  </div>
</div>
